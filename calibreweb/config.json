{
    "name": "Calibre Web",
    "version": "0.3",
    "slug": "calibre-web",
    "description": "Calibre web",
    "url": "https://github.com/gleepwurp/hassio-addons/tree/master/bicbucstriim",
    "startup": "after",
    "boot": "auto",
    "image": "bestlibre/{arch}-calibre-web",
    "arch": ["armv7", "armhf", "amd64", "aarch64", "i386"],
    "options": {
        "calibre_library": "/share",
        "SSL Certfile": "",
        "SSL keyfile": ""
    },
    "schema": {
        "calibre_library": "str"
    },
    "ports": {"8083/tcp": "8083"},
    "ports_description": {"8083/tcp": "Web Interface"},
    "map": ["share:rw"],
    "webui": "http://[HOST]:[PORT:8083]"
}
